{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "nodemon src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "test": "npx tsc && jasmine",
    "test-db": "cross-env ENV=test yarn test-migrate",
    "test-migrate": "npx tsc && db-migrate --env test up && jasmine && db-migrate reset --env test",
    "tsc": "tsc",
    "build": "npx tsc",
    "db-up": "db-migrate up",
    "db-down": "db-migrate down",
    "jasmine": "jasmine",
    "prettier": "prettier --write src/**/*.ts",
    "lint": "eslint . --ext .ts"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.19.0",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.3",
    "eslint": "^8.0.1",
    "express": "^4.17.1",
    "jasmine": "^4.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "nodemon": "^2.0.20",
    "pg": "^8.9.0",
    "prettier": "^2.8.3",
    "supertest": "^6.3.3",
    "typescript": "*"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.9",
    "@types/jasmine": "^4.3.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.11.18",
    "@types/pg": "^7.14.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint-config-standard-with-typescript": "^33.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "ts-node": "^10.9.1",
    "tsc-watch": "^4.2.9"
  }
}
